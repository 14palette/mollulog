{
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "codegen": "graphql-codegen",
    "dev": "react-router dev --host",
    "build": "react-router build",
    "start": "wrangler pages dev ./build/client",
    "predeploy": "pnpm run build",
    "deploy": "wrangler pages deploy ./build/client --branch main",
    "assets:build": "cd lambda && GOOS=linux GOARCH=amd64 go build -tags lambda.norpc -o build/bootstrap ./assets-metadata",
    "assets:plan": "tofu -chdir=deploy/assets plan",
    "assets:deploy": "tofu -chdir=deploy/assets apply",
    "db:dev:migrate": "wrangler d1 migrations apply --local DB",
    "db:prod:migrate": "wrangler d1 migrations apply --remote DB",
    "test": "jest",
    "typecheck": "react-router typegen && tsc",
    "lint": "biome lint --write"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.4.0",
    "@graphql-codegen/cli": "^5.0.5",
    "@headlessui/react": "^2.2.2",
    "@heroicons/react": "^2.2.0",
    "@react-router/cloudflare": "^7.5.2",
    "@react-router/fs-routes": "^7.5.2",
    "@sentry/cloudflare": "^9.14.0",
    "@simplewebauthn/browser": "^11.0.0",
    "@simplewebauthn/server": "^11.0.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@urql/exchange-request-policy": "^1.2.1",
    "dayjs": "^1.11.13",
    "drizzle-orm": "^0.43.1",
    "fido2-lib": "^3.5.3",
    "graphql": "^16.11.0",
    "hangul-js": "^0.2.6",
    "isbot": "^5.1.27",
    "nanoid": "^3.3.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.5.2",
    "react-top-loading-bar": "^3.0.2",
    "react-youtube": "^10.1.0",
    "remix-auth": "^3.7.0",
    "remix-auth-google": "^2.0.0",
    "urql": "^4.2.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.27.0",
    "@biomejs/biome": "^1.9.4",
    "@cloudflare/workers-types": "^4.20250427.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.25.1",
    "@jest/globals": "^29.7.0",
    "@react-router/dev": "^7.5.2",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "drizzle-kit": "^0.31.0",
    "jest": "^29.7.0",
    "tailwindcss": "^4.1.4",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.13.2"
  },
  "engines": {
    "node": ">=22.14.0"
  },
  "pnpm": {
    "overrides": {
      "esbuild@<=0.24.2": ">=0.25.0"
    }
  }
}