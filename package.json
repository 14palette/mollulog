{
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "codegen": "graphql-codegen",
    "dev": "react-router dev --host",
    "start": "wrangler dev",
    "staging:build": "CLOUDFLARE_ENV=staging react-router build --mode staging",
    "staging:deploy": "pnpm run staging:build && wrangler deploy",
    "prod:build": "CLOUDFLARE_ENV=production react-router build --mode production",
    "prod:deploy": "pnpm run prod:build && wrangler deploy",
    "assets:build": "cd lambda && GOOS=linux GOARCH=amd64 go build -tags lambda.norpc -o build/bootstrap ./assets-metadata",
    "assets:plan": "tofu -chdir=deploy/assets plan",
    "assets:deploy": "tofu -chdir=deploy/assets apply",
    "dev:db:migrate": "wrangler d1 migrations apply --local DB",
    "prod:db:migrate": "wrangler d1 migrations apply --remote DB",
    "test": "jest",
    "typecheck": "react-router typegen && tsc",
    "lint": "biome lint --write"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.4.0",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@react-router/cloudflare": "^7.7.1",
    "@react-router/fs-routes": "^7.7.1",
    "@simplewebauthn/browser": "^11.0.0",
    "@simplewebauthn/server": "^11.0.0",
    "dayjs": "^1.11.13",
    "drizzle-orm": "^0.43.1",
    "fido2-lib": "^3.5.3",
    "graphql": "^16.11.0",
    "hangul-js": "^0.2.6",
    "isbot": "^5.1.29",
    "marked": "^15.0.12",
    "nanoid": "^3.3.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-loading-skeleton": "^3.5.0",
    "react-router": "^7.7.1",
    "react-top-loading-bar": "^3.0.2",
    "react-youtube": "^10.1.0",
    "remix-auth": "^3.7.0",
    "remix-auth-google": "^2.0.0",
    "sanitize-html": "^2.17.0",
    "urql": "^4.2.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.27.1",
    "@biomejs/biome": "^1.9.4",
    "@cloudflare/vite-plugin": "^1.11.0",
    "@cloudflare/workers-types": "^4.20250803.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@jest/globals": "^29.7.0",
    "@react-router/dev": "^7.7.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.4",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "drizzle-kit": "^0.31.0",
    "jest": "^29.7.0",
    "tailwindcss": "^4.1.11",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vite": "^6.3.6",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.27.0"
  },
  "engines": {
    "node": ">=22.14.0"
  },
  "pnpm": {
    "overrides": {
      "esbuild@<=0.24.2": ">=0.25.0"
    }
  }
}