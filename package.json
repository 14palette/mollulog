{
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "codegen": "graphql-codegen",
    "dev": "remix vite:dev --host",
    "build": "remix vite:build",
    "start": "wrangler pages dev ./build/client",
    "predeploy": "pnpm run build",
    "deploy": "wrangler pages deploy ./build/client --branch main",
    "assets:build": "cd lambda && GOOS=linux GOARCH=amd64 go build -tags lambda.norpc -o build/bootstrap ./assets-metadata",
    "assets:plan": "tofu -chdir=deploy/assets plan",
    "assets:deploy": "tofu -chdir=deploy/assets apply",
    "db:dev:migrate": "wrangler d1 migrations apply --local DB",
    "db:prod:migrate": "wrangler d1 migrations apply --remote DB",
    "test": "jest",
    "typecheck": "tsc",
    "lint": "eslint --fix app/"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.3.4",
    "@graphql-codegen/cli": "^5.0.5",
    "@headlessui/react": "^2.2.1",
    "@heroicons/react": "^2.2.0",
    "@remix-run/cloudflare": "^2.16.4",
    "@remix-run/cloudflare-pages": "^2.16.4",
    "@remix-run/css-bundle": "^2.16.4",
    "@remix-run/react": "^2.16.4",
    "@simplewebauthn/browser": "^11.0.0",
    "@simplewebauthn/server": "^11.0.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@urql/exchange-request-policy": "^1.2.1",
    "dayjs": "^1.11.13",
    "drizzle-orm": "^0.39.3",
    "fido2-lib": "^3.5.3",
    "graphql": "^16.10.0",
    "hangul-js": "^0.2.6",
    "isbot": "^3.8.0",
    "nanoid": "^3.3.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-youtube": "^10.1.0",
    "remix-auth": "^3.7.0",
    "remix-auth-google": "^2.0.0",
    "urql": "^4.2.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.27.0",
    "@cloudflare/workers-types": "^4.20250407.0",
    "@jest/globals": "^29.7.0",
    "@remix-run/dev": "^2.16.4",
    "@remix-run/eslint-config": "^2.16.4",
    "@tailwindcss/postcss": "^4.1.3",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "drizzle-kit": "^0.30.6",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "tailwindcss": "^4.1.3",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^6.2.5",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.8.0"
  },
  "engines": {
    "node": ">=22.14.0"
  },
  "pnpm": {
    "overrides": {
      "cookie@<0.7.0": ">=0.7.0",
      "undici@>=6.0.0 <6.21.1": ">=6.21.1",
      "path-to-regexp@<0.1.12": ">=0.1.12",
      "esbuild@<=0.24.2": ">=0.25.0",
      "@babel/runtime@<7.26.10": ">=7.26.10",
      "@babel/helpers@<7.26.10": ">=7.26.10",
      "tar-fs@>=2.0.0 <2.1.2": ">=2.1.2"
    }
  }
}
